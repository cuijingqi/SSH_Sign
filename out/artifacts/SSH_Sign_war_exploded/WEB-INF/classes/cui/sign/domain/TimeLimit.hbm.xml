<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cui.sign.domain">

    <class name="cui.sign.domain.TimeLimit" table="s_TimeLimit" dynamic-update="true" dynamic-insert="true">
        <id name="TLId">
            <generator class="native"></generator>
        </id>
        <property name="startTime" type="timestamp"></property>
        <property name="endTime" type="timestamp"></property>
    </class>
</hibernate-mapping>