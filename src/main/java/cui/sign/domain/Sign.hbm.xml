<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cui.sign.domain">

    <class name="cui.sign.domain.Sign" table="s_Sign" dynamic-update="true" dynamic-insert="true">
        <id name="signId">
            <generator class="native"></generator>
        </id>
        <property name="IP" type="string"></property>
        <property name="time" type="timestamp"></property>
        <many-to-one name="student" class="Student" column="student_id" lazy="false"></many-to-one>
    </class>
</hibernate-mapping>